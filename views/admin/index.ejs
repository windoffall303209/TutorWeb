<h1>Quản lý thông tin</h1>
<h2>Người dùng</h2>
<ul>
  <% users.forEach(user => { %>
  <li><%= user.username %> - <%= user.role %></li>
  <% }) %>
</ul>
<h2>Gia sư</h2>
<ul>
  <% tutors.forEach(tutor => { %>
  <li><%= tutor.full_name %></li>
  <% }) %>
</ul>
<h2>Lớp học</h2>
<ul>
  <% classes.forEach(cls => { %>
  <li><%= cls.grade %> - <%= cls.subject %> - <%= cls.status %></li>
  <% }) %>
</ul>
